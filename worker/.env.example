# Example .env file for worker
# Copy these values from your production environment

# Redis Queue
REDIS_URL=rediss://default:YOUR_REDIS_PASSWORD@your-redis-host.upstash.io:6379

# Database
DATABASE_URL=postgresql://user:password@host:5432/database
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Google OAuth Credentials
GOOGLE_OAUTH_CLIENT_ID=your_client_id.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET=GOCSPX-your_client_secret
GOOGLE_OAUTH_REFRESH_TOKEN=1//your_refresh_token

# Google Services
GOOGLE_SHEET_ID=your_spreadsheet_id
GOOGLE_DRIVE_FOLDER_ID=your_folder_id

# Telegram Bot (for notifications and error alerts)
# Bot Relawanns - untuk notif pendaftaran sukses
BOT_RELAWANNS_TOKEN=your_bot_relawanns_token
NOTIFICATION_CHAT_ID=your_chat_id

# Bot Admin - untuk worker error alerts
TELEGRAM_BOT_TOKEN=your_bot_admin_token
TELEGRAM_CHAT_ID=your_admin_telegram_id

# Environment
NODE_ENV=production
