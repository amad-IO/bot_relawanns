# Environment Variables untuk Docker Compose
# Copy file ini ke .env dan isi dengan nilai sebenarnya

# Bot Tokens
BOT_ADMIN_TOKEN=your_bot_admin_token_here
BOT_RELAWANNS_TOKEN=your_bot_relawanns_token_here

# Database (bisa pakai Supabase atau PostgreSQL lokal)
DATABASE_URL=postgresql://user:password@host:5432/database

# Admin Telegram IDs (pisahkan dengan koma jika lebih dari 1)
ADMIN_TELEGRAM_IDS=123456789,987654321
